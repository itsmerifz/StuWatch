<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.itsmerifz.stuwatch">

  <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
  <uses-permission android:name="android.permission.INTERNET" />

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/Theme.AppCompat.Light">
    <activity android:name=".MenuHasilSiswa"></activity>
    <activity android:name=".HasilSiswa" />
    <activity
        android:name=".MenuInputData"
        android:label="Input Data"
        android:parentActivityName=".InputDataa" />
    <activity
        android:name=".InputDataa"
        android:parentActivityName=".MainActivity" />
    <activity
        android:name=".TambahSiswa"
        android:parentActivityName=".DaftarSiswa" />
    <activity
        android:name=".EditSiswa"
        android:parentActivityName=".DaftarSiswa" />
    <activity
        android:name=".DetailSiswa"
        android:parentActivityName=".DaftarSiswa" />
    <activity
        android:name=".DaftarSiswa"
        android:parentActivityName=".MainActivity" />
    <activity
        android:name=".EditProfil"
        android:label="Edit Profil"
        android:parentActivityName=".MainActivity" />
    <activity android:name=".LoginAkun">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity
        android:name=".DaftarAkun"
        android:label="Pendaftaran"
        android:parentActivityName=".LoginAkun" />
    <activity android:name=".MainActivity" />

    <meta-data
        android:name="preloaded_fonts"
        android:resource="@array/preloaded_fonts" />
  </application>

</manifest>